{
  "agents": {
    "defaults": {
      "model": {
        "primary": "ollama/gpt-oss:20b",
        "fallbacks": [
          "ollama/llama3.2:latest"
        ]
      }
    }
  },
  "models": {
    "providers": {
      "ollama": {
        "baseUrl": "${OLLAMA_BASE_URL}",
        "apiKey": "${OLLAMA_API_KEY}",
        "api": "openai-completions",
        "models": [
          {
            "id": "gpt-oss:20b",
            "name": "gpt-oss:20b"
          },
          {
            "id": "starcoder2:15b",
            "name": "starcoder2:15b"
          },
          {
            "id": "phi4:14b",
            "name": "phi4:14b"
          },
          {
            "id": "llama3.2:latest",
            "name": "llama3.2:latest"
          }
        ]
      }
    }
  },
  "channels": {},
  "gateway": {
    "mode": "local",
    "auth": {
      "mode": "token",
      "token": "${OPENCLAW_GATEWAY_TOKEN}"
    }
  }
}
