{
  "messages": {
    "ackReactionScope": "group-mentions"
  },
  "agents": {
    "defaults": {
      "skipBootstrap": true,
      "model": {
        "primary": "ollama/gpt-oss:20b",
        "fallbacks": [
          "ollama/llama3.2:latest"
        ]
      },
      "maxConcurrent": 4,
      "subagents": {
        "maxConcurrent": 8
      },
      "compaction": {
        "mode": "safeguard"
      },
      "workspace": "/home/node/.openclaw/workspace"
    }
  },
  "models": {
    "providers": {
      "ollama": {
        "baseUrl": "${OLLAMA_BASE_URL}",
        "apiKey": "${OLLAMA_API_KEY}",
        "api": "openai-completions",
        "models": [
          {
            "id": "gpt-oss:20b",
            "name": "gpt-oss:20b"
          },
          {
            "id": "starcoder2:15b",
            "name": "starcoder2:15b"
          },
          {
            "id": "phi4:14b",
            "name": "phi4:14b"
          },
          {
            "id": "llama3.2:latest",
            "name": "llama3.2:latest"
          }
        ]
      }
    }
  },
  "commands": {
    "native": "auto",
    "nativeSkills": "auto"
  },
  "channels": {
    "imessage": {
      "enabled": true,
      "dmPolicy": "pairing",
      "groupPolicy": "allowlist"
    }
  },
  "plugins": {
    "entries": {
      "imessage": {
        "enabled": true
      }
    }
  },
  "gateway": {
    "mode": "local",
    "auth": {
      "mode": "token",
      "token": "${OPENCLAW_GATEWAY_TOKEN}",
      "rateLimit": {
        "maxAttempts": 10,
        "windowMs": 60000,
        "lockoutMs": 300000,
        "exemptLoopback": true
      }
    },
    "port": 18789,
    "bind": "tailnet",
    "tailscale": {
      "mode": "serve",
      "resetOnExit": true
    }
  },
  "wizard": {},
  "meta": {}
}
