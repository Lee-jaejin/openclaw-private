{
  "agents": {
    "defaults": {
      "model": {
        "primary": "ollama/llama3.3:latest",
        "fallbacks": [
          "ollama/codellama:34b",
          "ollama/llama3.2:latest"
        ]
      }
    }
  },
  "models": {
    "providers": {
      "ollama": {
        "baseUrl": "${OLLAMA_BASE_URL}",
        "apiKey": "${OLLAMA_API_KEY}",
        "api": "openai-completions"
      }
    }
  },
  "gateway": {
    "mode": "local",
    "auth": {
      "mode": "token",
      "token": "${OPENCLAW_GATEWAY_TOKEN}"
    }
  }
}
